{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "buildCommand": "cd Chatbox-gemini && npm install && npm run build",
  "outputDirectory": "Chatbox-gemini/.next",
  "installCommand": "cd Chatbox-gemini && npm install",
  "rootDirectory": "Chatbox-gemini",
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "NEXTAUTH_SECRET": "@nextauth_secret"
  }
}
